      "enter no. of elements:5\n",
      "enter element: 1\n",
      "enter element: 2\n",
      "enter element: 3\n",
      "enter element: 4\n",
      "enter element: 5\n",
      "Orginal stack is :  ['1', '2', '3', '4', '5']\n",
      "reversed stack is :  ['5', '4', '3', '2', '1']\n"
     ]
    }
   ],
   "source": [
    "stack=[]\n",
    "\n",
    "n=int(input(\"enter no. of elements:\"))\n",
    "for i in range(n):\n",
    "    val=input(\"enter element: \")\n",
    "    stack.append(val)\n",
    "print(\"Orginal stack is : \",stack)\n",
    "revstack=[]\n",
    "while stack:\n",
    "    val=stack.pop()\n",
    "    revstack.append(val)\n",
    "\n",
    "stack=revstack\n",
    "print(\"reversed stack is : \",stack)"
   ]
  },
