   "source": [
    "str = \"ab+c*\"\n",
    "stack = []\n",
    "for i in str:\n",
    "    if i.isalnum():\n",
    "        stack.append(i)\n",
    "    else:\n",
    "        oprnd2=stack.pop()\n",
    "        oprnd1=stack.pop()\n",
    "        res=i+oprnd1+oprnd2\n",
    "        stack.append(res)\n",
    "print(stack[-1])"
   ]
